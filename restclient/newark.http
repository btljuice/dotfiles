# Environment
:newark_url = localhost:7070

# Prediction / Get Tickets
:origin = "FLL"
:destination = "GHC"
:departure_date = "2019-07-01"
POST http://:newark_url/api/v2/prediction
Content-Type: application/json
# Need to steal this from the frontend simulator
H-Request: 
{
	"alertKey": {
		"filter": {
			"TripFilter": "NoFilter"},
		"grouping": {
			"TripGrouping": "DateGrouping",
			"route": {
				"destination": {
					"code": :destination ,
					"regionType": "airport"
				},
				"origin": {
					"code": :origin ,
					"regionType": "airport"
				}
			},
			"departureDate": :departure_date
		}
	},
	"isOnboarding": false,
	"shopPassengers": {
		"Adult": 1
	},
	"autoWatch": false
}
